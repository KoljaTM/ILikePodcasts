<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
    "-//Puppy Crawl//DTD Suppressions 1.1//EN"
    "http://www.puppycrawl.com/dtds/suppressions_1_1.dtd">

<suppressions>

    <!--  generated classes -->
	<suppress checks="[a-zA-Z0-9]*" files="\w*_\.java" /> <!-- Generated classes by AndroidAnnotations -->
	<suppress checks="[a-zA-Z0-9]*" files="\w*BuildConfig\.java" /> <!-- Generated by Android -->
    
    <!-- IMPORTS -->
	<suppress checks="AvoidStaticImport" files="\w*Test\.java" /> <!-- JUnit4 test classes need to import asserts, matchers and builders -->
    <suppress checks="AvoidStaticImport" files="\w*Builder\.java" /> <!-- test data builders are allowed to import other builders -->

</suppressions>